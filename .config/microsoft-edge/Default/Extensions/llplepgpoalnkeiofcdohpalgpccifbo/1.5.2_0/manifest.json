{
   "author": "Patryk RzucidÅ‚o [@ptkdev] \u003Csupport@ptkdev.io> (https://ptk.dev)",
   "background": {
      "persistent": false,
      "scripts": [ "js/commands.min.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon_app_128x128.png",
      "default_popup": "popup.html",
      "default_title": "Aspect Ratio 21:9"
   },
   "commands": {
      "toggle-crop": {
         "description": "Force crop/zoom mode",
         "global": true
      },
      "toggle-force-219": {
         "description": "Force 21:9 aspect ratio",
         "global": true
      },
      "toggle-force-329": {
         "description": "Force 32:9 aspect ratio",
         "global": true
      },
      "toggle-force-custom": {
         "description": "Force CUSTOM aspect ratio",
         "global": true
      },
      "toggle-force-default": {
         "description": "Force 16:9 aspect ratio",
         "global": true
      },
      "toggle-fullscreen-mode": {
         "description": "Toggle \"only in fullscreenmode\"",
         "global": true
      },
      "toggle-stretch": {
         "description": "Force stretch mode",
         "global": true
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/video.min.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "images/icon_app_128x128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_extName__",
   "options_page": "options.html",
   "permissions": [ "storage" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "1.5.2"
}
